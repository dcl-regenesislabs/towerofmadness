{
  "name": "tower-of-madness",
  "version": "1.0.0",
  "description": "Tower climbing multiplayer game for Decentraland",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcl-regenesislabs/towerofmadness.git"
  },
  "scripts": {
    "start": "sdk-commands start",
    "deploy": "sdk-commands deploy",
    "deploy:testing": "sdk-commands deploy --target-content https://worlds-content-server.decentraland.zone",
    "deploy:production": "sdk-commands deploy --target-content https://worlds-content-server.decentraland.org",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next"
  },
  "devDependencies": {
    "@dcl/asset-packs": "latest",
    "@dcl/sdk": "https://sdk-team-cdn.decentraland.org/@dcl/js-sdk-toolchain/branch/fix/unify-storage-command/dcl-sdk-7.19.1-21496291120.commit-bc3be00.tgz"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  }
}
